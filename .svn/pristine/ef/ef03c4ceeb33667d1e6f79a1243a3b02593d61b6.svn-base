  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.LocalDbConnectionFactory, EntityFramework">
      <parameters>
        <parameter value="v12.0" />
      </parameters>
    </defaultConnectionFactory>
  </entityFramework>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="serviceSoap" allowCookies="true" maxBufferPoolSize="20000000"
          maxReceivedMessageSize="20000000">
          <security mode="Transport" />
        </binding>
        <binding name="SMSWebServiceSoap" />
        <binding name="serviceSoap1" />
        <binding name="serviceSoap2">
          <security mode="Transport" />
        </binding>
        <binding name="serviceSoap3" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://192.168.2.104/SMSWebService/SMSWebService.asmx"
        binding="basicHttpBinding" bindingConfiguration="SMSWebServiceSoap"
        contract="SMS.SMSWebServiceSoap" name="SMSWebServiceSoap" />
      <endpoint address="https://pgas.ph/epsws/service.asmx" binding="basicHttpBinding"
        bindingConfiguration="serviceSoap" contract="epsws.serviceSoap"
        name="serviceSoap" />
      <endpoint address="https://pgas.ph/epsws/service.asmx" binding="basicHttpBinding"
        bindingConfiguration="serviceSoap2" contract="transmittal_ws.serviceSoap"
        name="serviceSoap1" />
    </client>
